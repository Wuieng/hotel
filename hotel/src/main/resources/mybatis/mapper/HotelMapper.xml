<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cxd.mapper.HotelMapper">
    <select id="queryHotelList" resultType="Hotel">
        select * from hotelinfo
    </select>

    <select id="queryHotelById" resultType="Hotel">
        select * from hotelinfo where Id = #{Id}
    </select>


</mapper>