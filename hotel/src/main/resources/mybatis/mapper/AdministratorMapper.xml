<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cxd.mapper.AdministratorMapper">
    <select id="queryAdministratorList" resultType="Administrator">
        select * from administrator
    </select>

    <select id="queryAdministratorById" resultType="Administrator">
        select * from administrator where Id = #{Id}
    </select>


</mapper>